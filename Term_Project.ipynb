   "source": [
    "#Let's create a pivot table to see the median values of Temp for each Month and Year \n",
    "weather_canada.pivot_table('Temp (°C)', index='Month', columns='Year', aggfunc=np.median) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 209,
   "metadata": {},
   "outputs": [],
   "source": [
    "Median_Temp = weather_canada.groupby(['Month', 'Year'])['Temp (°C)'].transform(lambda x: x.fillna(x.median()))"
   ]
  },
